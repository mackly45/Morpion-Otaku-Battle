{
  "version": 2,
  "builds": [
    {
      "src": "view/**",
      "use": "@vercel/static"
    },
    {
      "src": "static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/",
      "dest": "/view/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/view/$1"
    }
  ]
}